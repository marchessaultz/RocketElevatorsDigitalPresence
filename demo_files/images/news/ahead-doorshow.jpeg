        captchakeyelem.value = captchakey;
        captchaStartTime = new Date().getTime();
    }

    function captchaTimer() {
        var now = new Date().getTime();
        if ((now - captchaStartTime) > refreshAfter) {
            captchaRefresh();
        }
        var captchatimer = document.getElementById("cq_captchatimer");
        if (!captchatimer) {
            // captcha has been removed
            return;
        }
        var percentage = (100 - (now - captchaStartTime) / refreshAfter * 100).toFixed(1);
        captchatimer.style.width = percentage + "%";
    }

    setInterval(captchaTimer, 500);
</script>

